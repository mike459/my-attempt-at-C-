using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int size = 40;
            double price = 15.0
            Console.WriteLine((size != 200) || (price >= 10.99));


            string Age;
            int age;

            Console.WriteLine("Please enter your childs age.");
            Age = Console.ReadLine();
            age = Convert.ToInt32(Age);

            if (age > 18)
            {
                Console.WriteLine(" Your child can drive on their own.");
            }
            else if ((age < 18) && (age > 16))
            {
                Console.WriteLine("Your child can drive WITH permission.");
            }
            else
            {
                Console.WriteLine("Your child can not drive.");
            }


            int first = 5;
            int sec = 3;

            if (first == sec)
            {
                Console.WriteLine("These two numbers are equal");
            }
            else
            {
                if (first > sec)
                {
                    Console.WriteLine("The first number is greater");
                }
                else
                {
                    Console.WriteLine("The second number is greater");
                }

            string Age;
            
            int age;

            Console.WriteLine("Type in your age.");
            Age = Console.ReadLine();
            age = Convert.ToInt32(Age);
            if ((age >= 5) && (age <= 10))
            {
                Console.WriteLine(" You are in Elementary school");
            }
            else if ((age >= 11) && (age <= 13))
            {
                Console.WriteLine(" You are in Middle school.");
            }
            else if ((age >= 13) && (age <= 17))
            {
                Console.WriteLine("You are in High school.");
            }
            else if ((age >= 18))
            {
                Console.WriteLine("You are wherever you are.");
            }


            int counter = 0;
            do
            {
                Console.WriteLine("The counter value is: " + counter);
                counter++;
            }
            while (counter <= 9);


            for (int i = 0; i <= 10; i ++)
            {
                Console.WriteLine("The valueof i = " + i);
            }

        }
    }
}
